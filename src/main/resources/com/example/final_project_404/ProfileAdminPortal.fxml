<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.final_project_404.ProfileAdminPortalController">
    <children>
        <HBox fx:id="headerHBox" prefHeight="80.0" prefWidth="800.0" styleClass="header" stylesheets="@EmployeePortal.css">
            <children>
                <Label styleClass="header-title" stylesheets="@EmployeePortal.css" text="Admin Portal" />
            <Label styleClass="header-title" text=" --&gt; Profile" />
            </children>
        </HBox>
        <ScrollPane layoutY="80.0" prefHeight="420.0" prefWidth="170.0">
            <content>
                <VBox fx:id="navigationBarVBox" alignment="TOP_CENTER" prefHeight="420.0" prefWidth="150.0" styleClass="nav-bar" stylesheets="@EmployeePortal.css">
                    <children>
                        <Button layoutX="20.0" layoutY="20.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#dashboardAdminPortal" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Dashboard" />
                        <Button layoutX="20.0" layoutY="62.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#addFacultyAdminPortal" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Add Faculty" />
                        <Button layoutX="20.0" layoutY="65.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#updateFacultyAdminPortal" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Update&#10;Faculty" />
                        <Button minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#addDepartmentAdminPortal" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Add &#10;Department" textAlignment="CENTER" />
                        <Button layoutX="20.0" layoutY="176.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#updateDepartmentAdminPortal" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Update&#10;Department" textAlignment="CENTER" />
                        <Button minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#addMajorAdminPortal" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Add Major" textAlignment="CENTER" />
                        <Button layoutX="20.0" layoutY="307.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#updateMajorAdminPortal" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Update Major" textAlignment="CENTER" />
                        <Button minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#addEmployeeAdminPortal" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Add Employee" textAlignment="CENTER" />
                        <Button layoutX="20.0" layoutY="396.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#updateEmployeeAdminPortal" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Update&#10;Employee" textAlignment="CENTER" />
                        <Button layoutX="20.0" layoutY="150.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" stylesheets="@EmployeePortal.css" text="Profile">
                         <styleClass>
                            <String fx:value="nav-button" />
                            <String fx:value="active" />
                         </styleClass></Button>
                        <Button layoutX="20.0" layoutY="193.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#reportsAdminPortal" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Reports" />
                        <Button layoutX="20.0" layoutY="193.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#signOutAdminPortal" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Sign out" />
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </VBox>
            </content>
        </ScrollPane>
        <VBox fx:id="containerBarVBox" layoutX="170.0" layoutY="80.0" prefHeight="420.0" prefWidth="630.0" styleClass="content-box" stylesheets="@EmployeePortal.css">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
         <children>
            <HBox alignment="CENTER_LEFT">
               <children>
                  <Label style="-fx-text-fill: BLACK; -fx-font-weight: bold;" stylesheets="@EmployeePortal.css" text="USERNAME ">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="newUsername" prefHeight="30.0" prefWidth="300.0" promptText="username" stylesheets="@EmployeePortal.css">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="31.0" layoutY="31.0">
               <children>
                  <Label style="-fx-text-fill: BLACK; -fx-font-weight: bold;" stylesheets="@EmployeePortal.css" text="PASSWORD">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="newPassword" prefHeight="30.0" prefWidth="300.0" promptText="password" stylesheets="@EmployeePortal.css">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="31.0" layoutY="92.0">
               <children>
                  <Label alignment="CENTER" style="-fx-text-fill: BLACK; -fx-font-weight: bold;" stylesheets="@EmployeePortal.css" text="REPEAT&#10;PASSWORD" textAlignment="CENTER">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="repeatPassword" prefHeight="30.0" prefWidth="300.0" promptText="password (repeat)" stylesheets="@EmployeePortal.css">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
            <Button mnemonicParsing="false" onAction="#changeProfile" text="Submit">
               <VBox.margin>
                  <Insets bottom="10.0" top="40.0" />
               </VBox.margin>
            </Button>
         </children>
        </VBox>
    </children>
</AnchorPane>
