<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="550.0" minWidth="800" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.final_project_404.ReportsStudentPortalController">
    <children>
        <HBox fx:id="headerHBox" prefHeight="80.0" prefWidth="800.0" styleClass="header" stylesheets="@EmployeePortal.css">
            <children>
                <Label styleClass="header-title" stylesheets="@EmployeePortal.css" text="Student Portal" />
                <Label styleClass="header-title" text=" --&gt; Reports" />
            </children>
        </HBox>
      <ScrollPane layoutY="80.0" prefHeight="470.0" prefWidth="170.0">
         <content>
              <VBox fx:id="navigationBarVBox" alignment="TOP_CENTER" prefHeight="470.0" prefWidth="150.0" styleClass="nav-bar" stylesheets="@EmployeePortal.css">
                  <children>
                      <Button layoutX="20.0" layoutY="20.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#dashboardStudentPortal" styleClass="nav-button-dashboard" stylesheets="@DashboardButton.css" text="Dashboard" />
                      <Button layoutX="20.0" layoutY="150.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#profileStudentPortal" styleClass="nav-button-dashboard" stylesheets="@DashboardButton.css" text="Profile" />
                      <Button layoutX="20.0" layoutY="193.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" styleClass="nav-button-dashboard" stylesheets="@DashboardButton.css" text="Reports" />
                      <Button layoutX="20.0" layoutY="193.0" minHeight="55.0" minWidth="130.0" mnemonicParsing="false" onAction="#signOutStudentPortal" styleClass="nav-button-dashboard" stylesheets="@DashboardButton.css" text="Sign out" />
                  </children>
                  <padding>
                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </padding>
              </VBox>
         </content>
      </ScrollPane>
        <VBox fx:id="containerBarVBox" layoutX="169.0" layoutY="80.0" prefHeight="470.0" prefWidth="630.0" style="-fx-background-color: linear-gradient(from 100.0% 100.0% to 0.0% 0.0%, #00a1ffb1 0.0%, #ffffff 100.0%);" stylesheets="@EmployeePortal.css">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
