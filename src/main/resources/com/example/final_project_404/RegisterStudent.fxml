<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.final_project_404.RegisterStudentController">
    <children>
        <HBox fx:id="headerHBox" prefHeight="80.0" prefWidth="800.0" styleClass="header" stylesheets="@EmployeePortal.css">
            <children>
                <Label styleClass="header-title" stylesheets="@EmployeePortal.css" text="Employee Portal" />
            <Label styleClass="header-title" stylesheets="@EmployeePortal.css" text=" --&gt; Register New Student" />
            </children>
        </HBox>
        <VBox fx:id="navigationBarVBox" layoutY="80.0" prefHeight="420.0" prefWidth="150.0" styleClass="nav-bar" stylesheets="@EmployeePortal.css">
            <children>
                <Button layoutX="20.0" layoutY="20.0" mnemonicParsing="false" onAction="#dashboardEmployeePortal" prefHeight="31.0" prefWidth="150.0" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Dashboard" />
                <Button layoutX="20.0" layoutY="62.0" mnemonicParsing="false" onAction="#professorEmployeePortal" prefHeight="31.0" prefWidth="171.0" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Professors" />
                <Button mnemonicParsing="false" onAction="#RegisterNewProfessorEmployeePortal" prefHeight="50.0" prefWidth="130.0" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Register New&#10;Professor" textAlignment="CENTER" />
                <Button layoutX="20.0" layoutY="107.0" mnemonicParsing="false" onAction="#studentsEmpoyeePortal" prefHeight="31.0" prefWidth="171.0" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Students" />
                <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="231.0" stylesheets="@EmployeePortal.css" text="Register New&#10;Student" textAlignment="CENTER">
               <styleClass>
                  <String fx:value="nav-button" />
                  <String fx:value="active" />
               </styleClass></Button>
                <Button layoutX="20.0" layoutY="150.0" mnemonicParsing="false" onAction="#profileEmployeePortal" prefHeight="23.0" prefWidth="130.0" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Profile" />
                <Button layoutX="20.0" layoutY="193.0" mnemonicParsing="false" onAction="#reportsEmployeePortal" prefHeight="31.0" prefWidth="171.0" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Reports" />
                <Button layoutX="20.0" layoutY="193.0" mnemonicParsing="false" onAction="#signOutEmployeePortal" prefHeight="31.0" prefWidth="171.0" styleClass="nav-button" stylesheets="@EmployeePortal.css" text="Sign out" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
        <VBox fx:id="containerBarVBox" alignment="CENTER_LEFT" layoutX="150.0" layoutY="80.0" prefHeight="420.0" prefWidth="650.0" styleClass="content-box" stylesheets="@EmployeePortal.css">
            <padding>
                <Insets bottom="20.0" left="20.0" top="10.0" />
            </padding>
         <children>
            <TextField fx:id="firstnameRegisterStudentEmployee" maxHeight="0.0" maxWidth="300.0" prefHeight="26.0" prefWidth="200.0" promptText="First Name" stylesheets="@EmployeePortal.css">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="lastnameRegisterStudentEmployee" layoutX="31.0" layoutY="31.0" maxWidth="300.0" promptText="Last Name" stylesheets="@EmployeePortal.css">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="nationalidRegisterStudentEmployee" layoutX="31.0" layoutY="56.0" maxWidth="300.0" prefHeight="25.0" prefWidth="609.0" promptText="National ID" stylesheets="@EmployeePortal.css">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <ComboBox fx:id="genderChooserRegisterStudentEmployee" layoutX="31.0" layoutY="186.0" maxWidth="300.0" prefWidth="150.0" promptText="Gender" stylesheets="@comboBoxStyle.css">
               <VBox.margin>
                  <Insets bottom="7.0" />
               </VBox.margin>
            </ComboBox>
            <TextField fx:id="phoneNumberRegisterStudentEmployee" layoutX="31.0" layoutY="103.0" maxWidth="300.0" prefHeight="25.0" prefWidth="609.0" promptText="Phone Number" stylesheets="@EmployeePortal.css">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </TextField>
            <DatePicker fx:id="dateOfBirthRegisterStudentEmployee" maxWidth="300.0" onAction="#getDateOfBirth" promptText="Date of Birth" stylesheets="@EmployeePortal.css">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </DatePicker>
            <ComboBox fx:id="facultyChooserRegisterStudentEmployee" maxWidth="300.0" prefWidth="150.0" promptText="Faculty" stylesheets="@comboBoxStyle.css">
               <VBox.margin>
                  <Insets bottom="7.0" />
               </VBox.margin>
            </ComboBox>
            <ComboBox fx:id="departmentChooserRegisterStudentEmployee" layoutX="31.0" layoutY="133.0" maxWidth="300.0" prefWidth="150.0" promptText="Department" stylesheets="@comboBoxStyle.css">
               <VBox.margin>
                  <Insets bottom="7.0" />
               </VBox.margin>
            </ComboBox>
            <ComboBox fx:id="majorChooserRegisterStudentEmployee" layoutX="31.0" layoutY="173.0" maxWidth="300.0" prefWidth="150.0" promptText="Major" stylesheets="@comboBoxStyle.css">
               <VBox.margin>
                  <Insets bottom="7.0" />
               </VBox.margin>
            </ComboBox>
            <Button mnemonicParsing="false" onAction="#addNewStudent" stylesheets="@EmployeePortal.css" text="Register">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </Button>
         </children>
        </VBox>
    </children>
</AnchorPane>
